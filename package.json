{
  "name": "travel-packing-list",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://travel-packing-list.com",
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "prebuild": "yarn flow && yarn lint && yarn test",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "yarn test --coverage",
    "flow": "flow --max-warnings=0",
    "lint": "eslint .",
    "lint:fix": "eslint . --quiet --fix",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@kiwicom/orbit-components": "^0.115.0",
    "@kiwicom/orbit-design-tokens": "^0.13.7",
    "i18next": "^19.9.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.10",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.30.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-orbit-components": "^0.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "flow-bin": "^0.146.0",
    "flow-typed": "^3.3.1",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
